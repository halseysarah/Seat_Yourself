<%= form_for @restaurant do |f| %>
  <p>
    <%= f.label "Restaurant Name" %><br>
    <%= f.text_field "name" %>
  </p>
  <p>
    <%= f.label "Address" %><br>
    <%= f.text_field "address" %>
  </p>
  <p>
    <%= f.label "Phone Number" %><br>
    <%= f.text_field "phone_number" %>
  </p>
  <p>
    <%= f.label "Capacity" %><br>
    <%= f.text_field "capacity" %>
  </p>
  <p>
    <%= f.label "Summary" %><br>
    <%= f.text_field "summary" %>
  </p>
   <p>
    <%= f.label "Image URL" %><br>
    <%= f.text_field "image" %>
  </p>
  <p>
    <%= f.submit "Save" %> 
  </p>
<% end %>